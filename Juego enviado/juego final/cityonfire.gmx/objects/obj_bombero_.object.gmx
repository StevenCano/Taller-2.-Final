key_space = keyboard_check_pressed(vk_space);
key_down = keyboard_check(vk_down);

// Operaciones con las entradas de teclado
move = key_right - key_left;
hsp = move * movespeed;
if (vsp &lt; 10) vsp +=grav;
if (place_meeting(x,y+1,obj_block))
{
    if (key_space) vsp = -jumpspeed;
}

var hsp_final = hsp + hsp_carry;
hsp_carry = 0;

// Colisi贸n horizotal 
if (place_meeting(x+hsp_final, y, obj_block)){
    while(!place_meeting(x+sign(hsp_final), y, obj_block)){
         x += sign(hsp_final);
    }
    hsp_final = 0;
    hsp = 0; 
}
x += hsp_final;

if (vsp &lt; 10) {vsp += grav;}
if (place_meeting(x, y+1, obj_block)){
    vsp = key_space * -jumpspeed;
}

// Colisi贸n vertical 
if (place_meeting(x, y+vsp, obj_block)){
    while(!place_meeting(x, y+sign(vsp), obj_block)){
         y += sign(vsp);
    }
    vsp = 0; 
}
y += vsp;
//animaci贸n
if (hsp)
{ sprite_index = spr_bombero_

}

</string>
